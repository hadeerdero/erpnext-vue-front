s\r\n      if (error.response) {\r\n        // Check for ERPNext message format\r\n        if (error.response.data && error.response.data.message) {\r\n          if (typeof error.response.data.message === 'string') {\r\n            if (error.response.data.message.includes(\"Incorrect password\") || \r\n                error.response.data.message.includes(\"User disabled or missing\")) {\r\n              return \"Invalid email or password\";\r\n            }\r\n            return error.response.data.message;\r\n          } else if (error.response.data.message.error) {\r\n            return error.response.data.message.error;\r\n          }\r\n        }\r\n        \r\n        // Standard HTTP error messages\r\n        switch (error.response.status) {\r\n          case 401:\r\n            return \"Invalid email or password\";\r\n          case 403:\r\n            return \"Access denied\";\r\n          case 500:\r\n            return \"Server error. Please try again later.\";\r\n          default:\r\n            return \"Login failed. Please try again.\";\r\n        }\r\n      } else if (error.message) {\r\n        return error.message;\r\n      }\r\n      return \"An unexpected error occurred\";\r\n    }\r\n  },\r\n  mounted() {\r\n    // If already authenticated, redirect to home\r\n    if (this.isAuthenticated) {\r\n      this.$router.push(this.homePage || '/dashboard');\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Your existing styles remain unchanged */\r\n.boxed-size {\r\n  min-height: 100vh;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.main-content {\r\n  flex: 1;\r\n}\r\n\r\n.m-1230 {\r\n  max-width: 1230px;\r\n  width: 100%;\r\n}\r\n\r\n.mw-480 {\r\n  max-width: 480px;\r\n}\r\n\r\n.h-55 {\r\n  height: 55px;\r\n}\r\n\r\n.alert-danger {\r\n  color: #721c24;\r\n  background-color: #f8d7da;\r\n  border-color: #f5c6cb;\r\n  padding: 0.75rem 1.25rem;\r\n  border-radius: 0.25rem;\r\n}\r\n\r\n.btn-primary {\r\n  background-color: #3b7d